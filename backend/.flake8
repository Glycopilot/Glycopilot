[flake8]
max-line-length = 190
extend-ignore = E203, W503, E402
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    migrations,
    .pytest_cache,
    build,
    dist,
    staticfiles
per-file-ignores =
    __init__.py:F401
    asgi.py:E402
    wsgi.py:E402
    settings.py:F401,E501
    */tests.py:F401,F841,E501
    */test_*.py:F401,F841,E501
    apps/profiles/apps.py:F401

